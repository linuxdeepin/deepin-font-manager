<?xml version="1.0" encoding="UTF-8"?>
<schemalist>
        <schema path="/com/deepin/font-manager/" id="com.deepin.font-manager">
        <key name="use-global-pool" type="i">
            <default>0</default>
            <range min="0" max="1"></range>
            <summary>use global thread pool: false(0) and true(1)</summary>
        </key>
		<key name="max-thread-count" type="i">
			<default>-1</default>
			<range min="-1" max="100"></range>
			<summary>max-thread-count</summary>
		</key>
		<key name="export-max-thread-count" type="i">
			<default>-1</default>
			<range min="-1" max="100"></range>
			<summary>export-max-thread-count</summary>
		</key>
		<key name="install-max-thread-count" type="i">
			<default>-1</default>
			<range min="-1" max="100"></range>
			<summary>install-max-thread-count</summary>
		</key>
        <key name="sort-order" type="i">
            <default>1</default>
            <range min="0" max="1"></range>
            <summary>sort order: asc(0) and desc(1)</summary>
        </key>
	</schema>
</schemalist>

